#include<stdio.h>
int main() {
    int num1, num2, sum,sub,mul,division,modulus;

    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    sum = num1 + num2;
    sub=num1-num2;
    mul=num1*num2;
    division=num1/num2;
    modulus=num1%num2;

    printf("Sum: %d\n", sum);
    printf("Sub: %d\n", sub);
    printf("mul: %d\n", mul);
    printf("division: %d\n", division);
    printf("modulus: %d\n", modulus);

    return 0;
}

